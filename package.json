{
    "name": "easymde-demo",
    "version": "1.0.0",
    "description": "A demo website for Easy Markdown Editor",
    "scripts": {
        "build": "tsc --project tsconfig.json",
        "start": "cross-env NODE_ENV=production node ./dist/index.js",
        "serve": "gulp clean && npm run build && npm run start",
        "lint": "tslint --project tsconfig.json -c tslint.json \"src/**/*.ts\"",
        "test": "echo \"Error: no test specified\" && exit 1",
        "dev": "cross-env TS_NODE_FILES=true LEVEL=debug node --require ts-node/register ./src/index.ts"
    },
    "author": "Jeroen Akkerman",
    "license": "MIT",
    "dependencies": {
        "axios": "^0.18.0",
        "body-parser": "^1.18.3",
        "chalk": "^2.4.1",
        "compression": "^1.7.3",
        "express": "^4.16.4",
        "express-mysql-session": "^2.0.1",
        "express-serve-static-core": "^0.1.1",
        "express-session": "^1.15.6",
        "fs-extra": "^7.0.1",
        "gulp": "^4.0.0",
        "hbs": "^4.0.1",
        "hbs-utils": "0.0.4",
        "helmet": "^3.15.0",
        "ini": "^1.3.5",
        "mysql": "^2.16.0",
        "node-sass-middleware": "^0.11.0",
        "on-finished": "^2.3.0",
        "reflect-metadata": "^0.1.12",
        "source-map-support": "^0.5.9",
        "typescript": "^3.1.6",
        "winston-pnp-logger": "^1.0.5"
    },
    "devDependencies": {
        "@types/body-parser": "^1.17.0",
        "@types/compression": "0.0.36",
        "@types/express": "^4.16.0",
        "@types/express-mysql-session": "^1.2.2",
        "@types/express-serve-static-core": "^4.16.0",
        "@types/express-session": "^1.15.11",
        "@types/fs-extra": "^5.0.4",
        "@types/gulp": "^4.0.5",
        "@types/helmet": "0.0.42",
        "@types/ini": "^1.3.29",
        "@types/mysql": "^2.15.5",
        "@types/node": "^10.12.3",
        "@types/node-sass-middleware": "0.0.30",
        "@types/on-finished": "^2.3.1",
        "@types/source-map-support": "^0.4.1",
        "@types/winston": "^2.4.4",
        "cross-env": "^5.2.0",
        "ts-node": "^7.0.1",
        "tslint": "^5.11.0"
    }
}
