{
    "name": "easymde-demo",
    "version": "1.0.0",
    "description": "A demo website for Easy Markdown Editor",
    "scripts": {
        "build": "gulp clean && tsc --project tsconfig.json",
        "start": "cross-env NODE_ENV=production node ./dist/index.js",
        "serve": "npm run build && npm run start",
        "lint": "tslint --project tsconfig.json -c tslint.json \"src/**/*.ts\"",
        "test": "echo \"Error: no test specified\" && exit 1",
        "dev": "cross-env TS_NODE_FILES=true LEVEL=debug node --require ts-node/register ./src/index.ts"
    },
    "author": "Jeroen Akkerman",
    "license": "MIT",
    "dependencies": {
        "axios": "^0.18.0",
        "body-parser": "^1.18.3",
        "chalk": "^2.4.2",
        "compression": "^1.7.3",
        "express": "^4.16.4",
        "fs-extra": "^7.0.1",
        "gulp": "^4.0.0",
        "hbs": "^4.0.2",
        "hbs-utils": "0.0.4",
        "helmet": "^3.15.1",
        "ini": "^1.3.5",
        "node-sass-middleware": "^0.11.0",
        "on-finished": "^2.3.0",
        "reflect-metadata": "^0.1.13",
        "source-map-support": "^0.5.10",
        "typescript": "^3.3.3",
        "winston-pnp-logger": "^1.0.5"
    },
    "devDependencies": {
        "@types/body-parser": "^1.17.0",
        "@types/compression": "0.0.36",
        "@types/express": "^4.16.1",
        "@types/express-serve-static-core": "^4.16.1",
        "@types/fs-extra": "^5.0.5",
        "@types/gulp": "^4.0.5",
        "@types/helmet": "0.0.42",
        "@types/ini": "^1.3.30",
        "@types/node": "^11.9.4",
        "@types/node-sass-middleware": "0.0.31",
        "@types/on-finished": "^2.3.1",
        "@types/source-map-support": "^0.4.2",
        "@types/winston": "^2.4.4",
        "cross-env": "^5.2.0",
        "ts-node": "^8.0.2",
        "tslint": "^5.12.1"
    }
}
